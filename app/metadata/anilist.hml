              - CURRENT
              - PLANNING
              - COMPLETED
              - DROPPED
              - PAUSED
              - REPEATING
          aggregate_functions: {}
          comparison_operators: {}
        MediaRankType:
          representation:
            type: enum
            one_of:
              - RATED
              - POPULAR
          aggregate_functions: {}
          comparison_operators: {}
        MediaRelation:
          representation:
            type: enum
            one_of:
              - ADAPTATION
              - PREQUEL
              - SEQUEL
              - PARENT
              - SIDE_STORY
              - CHARACTER
              - SUMMARY
              - ALTERNATIVE
              - SPIN_OFF
              - OTHER
              - SOURCE
              - COMPILATION
              - CONTAINS
          aggregate_functions: {}
          comparison_operators: {}
        MediaSeason:
          representation:
            type: enum
            one_of:
              - WINTER
              - SPRING
              - SUMMER
              - FALL
          aggregate_functions: {}
          comparison_operators: {}
        MediaSort:

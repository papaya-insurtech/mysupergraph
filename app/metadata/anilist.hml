              - ID_DESC
              - MEDIA_ID
              - MEDIA_ID_DESC
              - TIME
              - TIME_DESC
              - EPISODE
              - EPISODE_DESC
          aggregate_functions: {}
          comparison_operators: {}
        Boolean:
          aggregate_functions: {}
          comparison_operators: {}
        CharacterRole:

              - ADMIN
              - LEAD_DEVELOPER
              - DEVELOPER
              - LEAD_COMMUNITY
              - COMMUNITY
              - DISCORD_COMMUNITY
              - LEAD_ANIME_DATA
              - ANIME_DATA
              - LEAD_MANGA_DATA
              - MANGA_DATA
              - LEAD_SOCIAL_MEDIA
              - SOCIAL_MEDIA
              - RETIRED
              - CHARACTER_DATA
              - STAFF_DATA
          aggregate_functions: {}
          comparison_operators: {}
        NotificationType:
          representation:
            type: enum
            one_of:
              - ACTIVITY_MESSAGE
              - ACTIVITY_REPLY
              - FOLLOWING
              - ACTIVITY_MENTION
              - THREAD_COMMENT_MENTION
              - THREAD_SUBSCRIBED
              - THREAD_COMMENT_REPLY
              - AIRING
              - ACTIVITY_LIKE
              - ACTIVITY_REPLY_LIKE
              - THREAD_LIKE
              - THREAD_COMMENT_LIKE
              - ACTIVITY_REPLY_SUBSCRIBED
              - RELATED_MEDIA_ADDITION
              - MEDIA_DATA_CHANGE
              - MEDIA_MERGE
              - MEDIA_DELETION
          aggregate_functions: {}
          comparison_operators: {}
        RecommendationRating:
          representation:
            type: enum
            one_of:
              - NO_RATING
              - RATE_UP
              - RATE_DOWN
          aggregate_functions: {}
          comparison_operators: {}
        RecommendationSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - RATING
              - RATING_DESC
          aggregate_functions: {}
          comparison_operators: {}
        ReviewRating:
          representation:
            type: enum
            one_of:
              - NO_VOTE
              - UP_VOTE
              - DOWN_VOTE
          aggregate_functions: {}
          comparison_operators: {}
        ReviewSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - SCORE
              - SCORE_DESC
              - RATING
              - RATING_DESC
              - CREATED_AT
              - CREATED_AT_DESC
              - UPDATED_AT
              - UPDATED_AT_DESC
          aggregate_functions: {}
          comparison_operators: {}
        RevisionHistoryAction:
          representation:
            type: enum
            one_of:
              - CREATE
              - EDIT
          aggregate_functions: {}
          comparison_operators: {}
        ScoreFormat:
          representation:
            type: enum
            one_of:
              - POINT_100
              - POINT_10_DECIMAL
              - POINT_10
              - POINT_5
              - POINT_3
          aggregate_functions: {}
          comparison_operators: {}
        SiteTrendSort:
          representation:
            type: enum
            one_of:
              - DATE
              - DATE_DESC
              - COUNT
              - COUNT_DESC
              - CHANGE
              - CHANGE_DESC
          aggregate_functions: {}
          comparison_operators: {}
        StaffLanguage:
          representation:
            type: enum
            one_of:
              - JAPANESE
              - ENGLISH
              - KOREAN
              - ITALIAN
              - SPANISH
              - PORTUGUESE
              - FRENCH
              - GERMAN
              - HEBREW
              - HUNGARIAN
          aggregate_functions: {}
          comparison_operators: {}
        StaffSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - ROLE
              - ROLE_DESC
              - LANGUAGE
              - LANGUAGE_DESC
              - SEARCH_MATCH
              - FAVOURITES
              - FAVOURITES_DESC
              - RELEVANCE
          aggregate_functions: {}
          comparison_operators: {}
        String:
          aggregate_functions: {}
          comparison_operators: {}
        StudioSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - NAME
              - NAME_DESC
              - SEARCH_MATCH
              - FAVOURITES
              - FAVOURITES_DESC
          aggregate_functions: {}
          comparison_operators: {}
        SubmissionSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
          aggregate_functions: {}
          comparison_operators: {}
        SubmissionStatus:
          representation:
            type: enum
            one_of:
              - PENDING
              - REJECTED
              - PARTIALLY_ACCEPTED
              - ACCEPTED
          aggregate_functions: {}
          comparison_operators: {}
        ThreadCommentSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
          aggregate_functions: {}
          comparison_operators: {}
        ThreadSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - TITLE
              - TITLE_DESC
              - CREATED_AT
              - CREATED_AT_DESC
              - UPDATED_AT
              - UPDATED_AT_DESC
              - REPLIED_AT
              - REPLIED_AT_DESC
              - REPLY_COUNT
              - REPLY_COUNT_DESC
              - VIEW_COUNT
              - VIEW_COUNT_DESC
              - IS_STICKY
              - SEARCH_MATCH
          aggregate_functions: {}
          comparison_operators: {}
        UserSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - USERNAME
              - USERNAME_DESC
              - WATCHED_TIME
              - WATCHED_TIME_DESC
              - CHAPTERS_READ
              - CHAPTERS_READ_DESC
              - SEARCH_MATCH
          aggregate_functions: {}
          comparison_operators: {}
        UserStaffNameLanguage:
          representation:
            type: enum
            one_of:
              - ROMAJI_WESTERN
              - ROMAJI
              - NATIVE
          aggregate_functions: {}
          comparison_operators: {}
        UserStatisticsSort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - COUNT
              - COUNT_DESC
              - PROGRESS
              - PROGRESS_DESC
              - MEAN_SCORE
              - MEAN_SCORE_DESC
          aggregate_functions: {}
          comparison_operators: {}
        UserTitleLanguage:
          representation:
            type: enum
            one_of:
              - ROMAJI
              - ENGLISH
              - NATIVE
              - ROMAJI_STYLISED
              - ENGLISH_STYLISED
              - NATIVE_STYLISED
          aggregate_functions: {}
          comparison_operators: {}
      object_types:
        ActivityLikeNotification:
          description: Notification for when a activity is liked
          fields:
            activity:
              description: The liked activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: ActivityUnion
            activityId:
              description: The id of the activity which was liked
              type:
                type: named
                name: Int
            context:
              description: The notification context text
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: String
            createdAt:
              description: The time the notification was created at
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            id:
              description: The id of the Notification
              type:
                type: named
                name: Int
            type:
              description: The type of notification
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: NotificationType
            user:
              description: The user who liked the activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: User
            userId:
              description: The id of the user who liked to the activity
              type:
                type: named
                name: Int
        ActivityMentionNotification:
          description: Notification for when authenticated user is @ mentioned in activity or reply
          fields:
            activity:
              description: The liked activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: ActivityUnion
            activityId:
              description: The id of the activity where mentioned
              type:
                type: named
                name: Int
            context:
              description: The notification context text
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: String
            createdAt:
              description: The time the notification was created at
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            id:
              description: The id of the Notification
              type:
                type: named
                name: Int
            type:
              description: The type of notification
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: NotificationType
            user:
              description: The user who mentioned the authenticated user
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: User
            userId:
              description: The id of the user who mentioned the authenticated user
              type:
                type: named
                name: Int
        ActivityMessageNotification:
          description: Notification for when a user is send an activity message
          fields:
            activityId:
              description: The id of the activity message
              type:
                type: named
                name: Int
            context:
              description: The notification context text
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: String
            createdAt:
              description: The time the notification was created at
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            id:
              description: The id of the Notification
              type:
                type: named
                name: Int
            message:
              description: The message activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: MessageActivity
            type:
              description: The type of notification
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: NotificationType
            user:
              description: The user who sent the message
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: User
            userId:
              description: The if of the user who send the message
              type:
                type: named
                name: Int
        ActivityReply:
          description: Replay to an activity item
          fields:
            activityId:
              description: The id of the parent activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            createdAt:
              description: The time the reply was created at
              type:
                type: named
                name: Int
            id:
              description: The id of the reply
              type:
                type: named
                name: Int
            isLiked:
              description: If the currently authenticated user liked the reply
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Boolean
            likeCount:
              description: The amount of likes the reply has
              type:
                type: named
                name: Int
            likes:
              description: The users who liked the reply
              type:
                type: nullable
                underlying_type:
                  type: array
                  element_type:
                    type: nullable
                    underlying_type:
                      type: named
                      name: User
            text:
              description: The reply text
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: String
              arguments:
                asHtml:
                  description: Return the string in pre-parsed html instead of markdown
                  type:
                    type: nullable
                    underlying_type:
                      type: named
                      name: Boolean
            user:
              description: The user who created reply
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: User
            userId:
              description: The id of the replies creator
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
        ActivityReplyLikeNotification:
          description: Notification for when a activity reply is liked
          fields:
            activity:
              description: The liked activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: ActivityUnion
            activityId:
              description: The id of the activity where the reply which was liked
              type:
                type: named
                name: Int
            context:
              description: The notification context text
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: String
            createdAt:
              description: The time the notification was created at
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            id:
              description: The id of the Notification
              type:
                type: named
                name: Int
            type:
              description: The type of notification
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: NotificationType
            user:
              description: The user who liked the activity reply
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: User
            userId:
              description: The id of the user who liked to the activity reply
              type:
                type: named
                name: Int
        ActivityReplyNotification:
          description: Notification for when a user replies to the authenticated users activity
          fields:
            activity:
              description: The liked activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: ActivityUnion
            activityId:
              description: The id of the activity which was replied too
              type:
                type: named
                name: Int
            context:
              description: The notification context text
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: String
            createdAt:
              description: The time the notification was created at
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            id:
              description: The id of the Notification
              type:
                type: named
                name: Int
            type:
              description: The type of notification
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: NotificationType
            user:
              description: The user who replied to the activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: User
            userId:
              description: The id of the user who replied to the activity
              type:
                type: named
                name: Int
        ActivityReplySubscribedNotification:
          description: Notification for when a user replies to activity the authenticated user has replied to
          fields:
            activity:
              description: The liked activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: ActivityUnion
            activityId:
              description: The id of the activity which was replied too
              type:
                type: named
                name: Int
            context:
              description: The notification context text
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: String
            createdAt:
              description: The time the notification was created at
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            id:
              description: The id of the Notification
              type:
                type: named
                name: Int
            type:
              description: The type of notification
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: NotificationType
            user:
              description: The user who replied to the activity
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: User
            userId:
              description: The id of the user who replied to the activity
              type:
                type: named
                name: Int
        AiringNotification:
          description: Notification for when an episode of anime airs
          fields:
            animeId:
              description: The id of the aired anime
              type:
                type: named
                name: Int
            contexts:
              description: The notification context text
              type:
                type: nullable
                underlying_type:
                  type: array
                  element_type:
                    type: nullable
                    underlying_type:
                      type: named
                      name: String
            createdAt:
              description: The time the notification was created at
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
            episode:
              description: The episode number that just aired
              type:
                type: named
                name: Int
            id:
              description: The id of the Notification
              type:
                type: named
                name: Int
            media:
              description: The associated media of the airing schedule
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Media
            type:
              description: The type of notification
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: NotificationType
        AiringProgression:
          description: Score & Watcher stats for airing anime by episode and mid-week
          fields:
            episode:
              description: The episode the stats were recorded at. .5 is the mid point between 2 episodes airing dates.
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Float
            score:
              description: The average score for the media
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Float
            watching:
              description: The amount of users watching the anime
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Int
        AiringSchedule:
          description: 'Media Airing Schedule. NOTE: We only aim to guarantee that FUTURE airing data is present and accurate.'
          fields:
            airingAt:
              description: The time the episode airs at
              type:
                type: named
                name: Int
            episode:
              description: The airing episode number
              type:
                type: named
                name: Int
            id:
              description: The id of the airing schedule item
              type:
                type: named
                name: Int
            media:
              description: The associate media of the airing episode
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Media
            mediaId:
              description: The associate media id of the airing episode
              type:
                type: named
                name: Int
            timeUntilAiring:
              description: Seconds until episode starts airing

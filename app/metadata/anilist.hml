              - MANGA
              - STAFF
          aggregate_functions: {}
          comparison_operators: {}
        ExternalLinkType:
          representation:
            type: enum
            one_of:
              - INFO
              - STREAMING
              - SOCIAL
          aggregate_functions: {}
          comparison_operators: {}
        Float:
          aggregate_functions: {}
          comparison_operators: {}
        FuzzyDateInt:
          aggregate_functions: {}
          comparison_operators: {}
        ID:
          aggregate_functions: {}
          comparison_operators: {}
        Int:
          aggregate_functions: {}
          comparison_operators: {}
        Json:
          aggregate_functions: {}
          comparison_operators: {}
        LikeableType:

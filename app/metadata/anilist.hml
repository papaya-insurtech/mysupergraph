kind: DataConnectorLink
version: v1
definition:
  name: anilist
  url:
    readWriteUrls:
      read:
        valueFromEnv: APP_ANILIST_READ_URL
      write:
        valueFromEnv: APP_ANILIST_WRITE_URL
  headers:
    Authorization:
      valueFromEnv: APP_ANILIST_AUTHORIZATION_HEADER
  schema:
    version: v0.1
    schema:
      scalar_types:
        ActivitySort:
          representation:
            type: enum
            one_of:
              - ID
              - ID_DESC
              - PINNED
          aggregate_functions: {}
          comparison_operators: {}
        ActivityType:
          representation:
            type: enum
            one_of:
              - TEXT
              - ANIME_LIST
              - MANGA_LIST
              - MESSAGE
              - MEDIA_LIST
          aggregate_functions: {}
          comparison_operators: {}
        AiringSort:
